{
  "name": "universus-simulator",
  "version": "0.1.0",
  "description": "Digital simulator for the Universus card game",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "api": "ts-node src/api-server.ts",
    "api:dev": "nodemon --watch src --ext ts --exec ts-node src/api-server.ts",
    "demo": "ts-node src/examples/demo.ts",
    "scrape": "ts-node src/scraper/scrape-all.ts",
    "example": "ts-node src/examples/example-load-cards.ts",
    "analyze-uvs-login": "ts-node src/scraper/analyze-uvs-ultra-login.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "universus",
    "card-game",
    "simulator",
    "tcg"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "puppeteer": "^24.31.0"
  }
}
